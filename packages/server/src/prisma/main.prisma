generator client {
  provider        = "prisma-client"
  previewFeatures = ["PrismaSchemaFolder", "relationJoins"]
  binaryTargets   = ["native", "linux-musl"]
  output          = "./../../node_modules/@prisma/generated"
}

datasource db {
  provider     = "postgresql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}
