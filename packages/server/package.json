{
  "name": "@akaiito/server",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "author": "anonymous",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "start": "NODE_ENV=production node ./bootstrap.js",
    "dev": "rimraf dist && cross-env NODE_ENV=local mwtsc --watch --run @midwayjs/mock/app && pnpm run prisma:seed",
    "test": "cross-env NODE_ENV=unittest jest",
    "cov": "jest --coverage",
    "lint": "mwts check",
    "lint:fix": "mwts fix",
    "ci": "npm run cov",
    "build": "pnpm run prisma:generate && mwtsc --cleanOutDir && node ./scripts/index.js",
    "upgrade": "midway-version -m -w",
    "prisma:generate": "prisma generate",
    "prisma:init": "pnpm prisma:push && pnpm prisma:seed",
    "prisma:pull": "prisma db pull",
    "prisma:push": "prisma db push",
    "prisma:seed": "prisma db seed",
    "prisma:update": "pnpm prisma:generate && pnpm prisma:push"
  },
  "dependencies": {
    "@akaiito/types": "workspace:*",
    "@akaiito/utils": "workspace:*",
    "@midwayjs/axios": "3.20.4",
    "@midwayjs/bootstrap": "3.20.4",
    "@midwayjs/busboy": "3.20.5",
    "@midwayjs/cache-manager": "3.20.4",
    "@midwayjs/captcha": "3.20.5",
    "@midwayjs/core": "3.20.4",
    "@midwayjs/decorator": "3.20.4",
    "@midwayjs/info": "3.20.5",
    "@midwayjs/jwt": "3.20.5",
    "@midwayjs/koa": "3.20.5",
    "@midwayjs/logger": "^3.4.2",
    "@midwayjs/security": "3.20.5",
    "@midwayjs/static-file": "3.20.5",
    "@midwayjs/validate": "3.20.5",
    "@prisma/client": "^6.7.0",
    "crypto-js": "^4.2.0",
    "fs-extra": "^11.3.0",
    "ip2region": "^2.3.0",
    "lodash": "^4.17.21",
    "yaml": "^2.7.1"
  },
  "devDependencies": {
    "@midwayjs/cli": "^2.1.1",
    "@midwayjs/mock": "3.20.4",
    "@prisma/adapter-pg": "^6.7.0",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/koa": "^2.15.0",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.15.14",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^8.0.0",
    "jest": "^29.7.0",
    "mwts": "^1.3.0",
    "mwtsc": "^1.15.1",
    "prisma": "^6.7.0",
    "ts-jest": "^29.3.2"
  },
  "midway-bin-clean": [
    ".vscode/.tsbuildinfo",
    "dist"
  ],
  "prisma": {
    "schema": "src/prisma",
    "seed": "pnpx ts-node src/prisma/seed.ts"
  }
}
