  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "start": "NODE_ENV=production node ./bootstrap.js",
    "dev": "cross-env NODE_ENV=local mwtsc --watch --run @midwayjs/mock/app && pnpm run prisma:seed",
    "test": "cross-env NODE_ENV=unittest jest",
    "cov": "jest --coverage",
    "lint": "mwts check",
    "lint:fix": "mwts fix",
    "ci": "npm run cov",
    "build": "pnpm run prisma:generate && mwtsc --cleanOutDir && node ./scripts/index.js",
    "prisma:generate": "prisma generate",
    "prisma:init": "pnpm prisma:push && pnpm prisma:seed",
    "prisma:pull": "prisma db pull",
    "prisma:push": "prisma db push",
    "prisma:seed": "prisma db seed",
    "prisma:update": "pnpm prisma:generate && pnpm prisma:push"
  },
  "dependencies": {
    "@auy/types": "workspace:*",
    "@auy/utils": "workspace:*",
    "@midwayjs/axios": "^3.19.0",
    "@midwayjs/bootstrap": "^3.19.0",
    "@midwayjs/busboy": "^3.19.0",
    "@midwayjs/cache-manager": "^3.19.0",
    "@midwayjs/captcha": "3.19.0",
    "@midwayjs/core": "^3.19.0",
    "@midwayjs/decorator": "^3.19.0",
    "@midwayjs/info": "^3.19.0",
    "@midwayjs/jwt": "3.19.0",
    "@midwayjs/koa": "^3.19.0",
    "@midwayjs/logger": "^3.4.2",
    "@midwayjs/security": "^3.19.0",
    "@midwayjs/static-file": "^3.19.0",
    "@midwayjs/validate": "^3.19.0",
    "@prisma/client": "^5.21.1",
    "crypto-js": "^4.2.0",
    "fs-extra": "^11.2.0",
    "ip2region": "^2.3.0",
    "lodash": "^4.17.21",
    "yaml": "^2.6.0"
  },
  "devDependencies": {
    "@midwayjs/cli": "^2.1.1",
    "@midwayjs/mock": "^3.19.0",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.13",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/koa": "^2.15.0",
    "@types/lodash": "^4.17.12",
    "@types/node": "^20.16.13",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.4.2",
    "jest": "^29.7.0",
    "mwts": "^1.3.0",
    "mwtsc": "^1.12.0",
    "prisma": "^5.21.1",
    "ts-jest": "^29.2.5"
  },
  "midway-bin-clean": [
    ".vscode/.tsbuildinfo",
    "dist"
  ],
  "prisma": {
    "schema": "prisma/schema",
    "seed": "pnpx ts-node prisma/seed.ts"
  }
}
