{
  "name": "@akaiito/server",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "dependencies": {
    "@akaiito/typings": "workspace:*",
    "@akaiito/utils": "workspace:*",
    "@midwayjs/bootstrap": "^3.17.0",
    "@midwayjs/captcha": "3.17.0",
    "@midwayjs/core": "^3.17.0",
    "@midwayjs/decorator": "^3.17.0",
    "@midwayjs/info": "^3.17.0",
    "@midwayjs/jwt": "3.17.0",
    "@midwayjs/koa": "^3.17.0",
    "@midwayjs/logger": "^3.4.2",
    "@midwayjs/static-file": "^3.17.0",
    "@midwayjs/busboy": "^3.17.0",
    "@midwayjs/validate": "^3.17.0",
    "@prisma/client": "^5.17.0",
    "fs-extra": "^11.2.0",
    "ip2region": "^2.3.0",
    "lodash": "^4.17.21",
    "yaml": "^2.4.5"
  },
  "devDependencies": {
    "@midwayjs/cli": "^2.1.1",
    "@midwayjs/mock": "^3.17.0",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/koa": "^2.15.0",
    "@types/lodash": "^4.17.7",
    "@types/node": "^20.14.11",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.4.2",
    "jest": "^29.7.0",
    "mwts": "^1.3.0",
    "mwtsc": "^1.11.1",
    "prisma": "^5.17.0",
    "ts-jest": "^29.2.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "start": "NODE_ENV=production node ./bootstrap.js",
    "dev": "cross-env NODE_ENV=local mwtsc --watch --run @midwayjs/mock/app && pnpm run prisma:seed",
    "test": "cross-env NODE_ENV=unittest jest",
    "cov": "jest --coverage",
    "lint": "mwts check",
    "lint:fix": "mwts fix",
    "ci": "npm run cov",
    "build": "pnpm run prisma:generate && mwtsc --cleanOutDir && node ./scripts/index.js",
    "prisma:generate": "prisma generate --schema ./prisma/schema",
    "prisma:init": "prisma db push && prisma db seed",
    "prisma:pull": "prisma db pull --schema ./prisma/schema",
    "prisma:push": "prisma db push --schema ./prisma/schema",
    "prisma:seed": "prisma db seed"
  },
  "midway-bin-clean": [
    ".vscode/.tsbuildinfo",
    "dist"
  ],
  "prisma": {
    "seed": "pnpx ts-node prisma/seed.ts",
    "schema": "prisma/schema/Main.prisma"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "anonymous",
  "license": "MIT"
}
