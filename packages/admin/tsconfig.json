{
	"extends": "@vue/tsconfig/tsconfig.web.json",
	"include": [
		"vite-env.d.ts",
		"vite/client",
		"src/**/*.ts",
		"src/**/*.d.ts",
		"src/**/*.tsx",
		"src/**/*.vue",
		"src/typings/auto-imports.d.ts",
		"src/typings/components.d.ts"
	],
	"compilerOptions": {
		"baseUrl": ".",
		"lib": [
			"es2017"
		],
		"paths": {
			"@/*": [
				"./src/*"
			],
			"~@/*": [
				"node_modules/@akaiito/typings/src/admin/*"
			]
		},
		"types": [
			"vite/client"
		]
	},
	"references": [
		{
			"path": "./tsconfig.config.json"
		}
	]
}
