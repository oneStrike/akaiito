{
  "name": "akaiito",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"pnpm -C ./packages/server dev\" \" pnpm -C ./packages/client dev:h5\"  \"pnpm -C ./packages/admin dev\"",
    "dev:server": "",
    "dev:admin": "pnpm -C ./packages/admin dev",
    "dev:client": "",
    "build": "",
    "lint:fix": "concurrently \"pnpm -C packages/admin lint\"  \"pnpm -C ./packages/server lint:fix\"  \"pnpm -C ./packages/client lint\"",
    "prettier": "prettier --config .prettierrc  --ignore-path .prettierignore --write ./**/**/*.{js,css,vue,ts,scss}",
    "test": "echo \"Error: no test specified\" && exit 1",
    "purge": "rimraf node_modules **/*/node_modules",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "ytt": "concurrently \"pnpm -C packages/typings ytt\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "commitizen": "^4.3.0",
    "concurrently": "^7.6.0",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3",
    "typescript": "~4.8.4"
  },
  "dependencies": {
    "axios": "^1.3.0",
    "dayjs": "^1.11.7",
    "lodash": "^4.17.21"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,ts,css,scss,vue,md}": [
      "pnpm run prettier",
      "pnpm run lint:fix"
    ]
  }
}
